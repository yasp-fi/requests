{
  "name": "@yasp/requests",
  "version": "0.0.0",
  "description": "yasp fi http requests package",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf node_modules",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --fix --cache ."
  },
  "keywords": [
    "typescript",
    "template",
    "package"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^18.6.4",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "./src/**/*.{ts,js}": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "author": "YaspFi",
  "license": "MIT",
  "dependencies": {
    "http-status-codes": "^2.2.0",
    "wretch": "^2.6.0"
  }
}
